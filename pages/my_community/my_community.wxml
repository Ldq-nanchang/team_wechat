<!--pages/my_community/my_community.wxml-->
<view class="container">
  <view class="top-tab">
    <wux-tabs wux-class="bordered" controlled current="{{ key }}" bindchange="onTabsChange">
        <block wx:for="{{ tabs }}" wx:key="key">
            <wux-tab key="{{ item.key }}" title="{{ item.title }}"></wux-tab>
        </block>
    </wux-tabs>
  </view>
  
  <view style="height:84rpx"></view>

  <swiper current="{{ index }}" bindchange="onSwiperChange" class="swiper">
      <block wx:key="tab1">
          <swiper-item>


              <view class="content">
                <myCommunity id="myCommunity"></myCommunity>
              </view>
          </swiper-item>
      </block>

      <block wx:key="tab2">
          <swiper-item>
              <view class="content">
                <!-- <myCommunity id="myCommunity"></myCommunity> -->
                    <wux-index height="100%" bind:change="onChange">
                        <wux-index-item wx:for="{{ alphabet }}" wx:key="" name="{{ item.initial }}">
                            <view class="demo-item" wx:for="{{ item.cells }}" wx:for-item="cell" wx:key="">{{ cell }}</view>
                        </wux-index-item>
                    </wux-index>
              </view>
          </swiper-item>
      </block>
  </swiper>

</view>
